{
  "name": "notre-dame",
  "version": "0.0.3",
  "description": "Hugo starter with a webpack development workflow",
  "repository": "ventizo/notre-dame",
  "main": "src/index.js",
  "scripts": {
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint src",
    "lint:styles": "stylelint \"src/styles/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint",
    "start": "run-p start:**",
    "start:hugo": "hugo -d ../dist -s site -vw",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "gc": "rimraf site/resources",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d ../dist -s site -v --minify",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.6",
    "@babel/register": "^7.6.2",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^6.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^6.6",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.1.0",
    "hugo-bin": "^0.47",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.2.0",
    "webpack": "^4.41",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "lazysizes": "^5.1.2"
  }
}
